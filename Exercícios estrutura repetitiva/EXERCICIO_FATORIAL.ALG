Algoritmo "Exercicio_fatorial"


Var

   i, numero, fatorial : inteiro
 


Inicio


      escreval ("Digite o valor de N:")
      leia (numero)

      fatorial <- 1

      para i de 1 ate numero faca
              fatorial <- fatorial * i
      fimpara

      
      escreval ("FATORIAL: ", fatorial)
      





Fimalgoritmo