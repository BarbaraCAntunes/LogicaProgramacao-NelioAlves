Algoritmo "Exercício Consumo"


Var

   distancia, combustivel, consumoMedio : real

Inicio

      escreval("Digite a distância percorrida:")
      leia(distancia)
      escreval("Digite o valor gasto com combustível")
      leia(combustivel)
      
      consumoMedio <- distancia / combustivel
      
      escreval("Consumo médio do carro:", consumoMedio:8:3)

Fimalgoritmo